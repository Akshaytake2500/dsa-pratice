class Solution {
    public int[] twoSum(int[] nums, int target) {
        for(int i=0;inums.length;i++){
           for(int j=i+1;jnums.length;j++){
            if(target==nums[i]+nums[j]){
                return new int[] {i,j};
            }
           }
        }
    return new int[] {};
    }
}
